    <body>
        <div class="row">
            <div id="navPanel" class="col-md-12 col-lg-12">
                <div class="pull-left">
                    <h5>Visualizador de fichas y planchetas</h5>
                </div>
                <div id="userData" class="pull-right">
                    <% if (locals.usuario) { %>
                        <p><%= usuario%>, <a href="#" id="btnLogout">Salir</a></p>
                    <% } %> 
                </div>
            </div>
        </div>
        <div class="row">
            <div id="sidePanel" class="col-md-3 col-lg-3">
                <div class="searchBox">
                    <div class="searchHead">
                        <div class="pull-left"><h5>Busequeda de ficha por cuenta</h5></div>
                        <div class="pull-right fillVertical"><button type="button" id='toggleFormFichaCuenta' class="btn btn-succes btn-md fillVertical toggleSearchBody"> Toggle </button></div>
                    </div>
                    <div id='cntFormFichaCuenta' class="searchBody">
                        <input name="cuenta" id="txtCuenta" class="form-control txtBox pull-left txtCuentaAFichaReq" maxlength="6" placeholder="n cuenta" required>
                        <button id="btnCuentaAFicha" type="button" class="fillHorizontal btn btn-default btn-md">Buscar</button>
                    </div>
                </div>
                <div class="searchBox">
                    <div class="searchHead">
                        <div class="pull-left"><h5>Busequeda de ficha por nomenclatura</h5></div>
                        <div class="pull-right fillVertical"><button type="button" id='toggleFormFichaNomen' class="btn btn-succes btn-md fillVertical toggleSearchBody"> Toggle </button></div>
                    </div>
                    <div id='cntFormFichaNomen' class="searchBody">
                        <input type="text" name="circunscripcion" id="txtCircunscripcion" maxlength="1" placeholder="Circ" class="form-control txtBox pull-left txtNomenAFichaReq" required>
                        <input type="text" name="seccion" id="txtSeccion" maxlength="1" placeholder="Sec" class="form-control txtBox pull-left txtNomenAFichaReq" required>
                        <input type="text" name="fraccion" id="txtFraccion" maxlength="3" placeholder="n frac" class="form-control txtBox pull-left">
                        <input type="text" name="letraFraccion" id="txtLFraccion" maxlength="1" placeholder="L frac" class="form-control txtBox pull-left">
                        <input type="text" name="manzana" id="txtManzana" maxlength="3" placeholder="n manz" class="form-control txtBox pull-left" >
                        <input type="text" name="letraManzana" id="txtLManzana" maxlength="1"placeholder="L manz" class="form-control txtBox pull-left">
                        <input type="text" name="parcela" id="txtParcela" maxlength="3" placeholder="n parc" class="form-control txtBox pull-left">
                        <input type="text" name="letraParcela" id="txtLParcela" maxlength="2" placeholder="L parcs" class="form-control txtBox pull-left">
                        <input type="text" name="uFuncional" id="txtUFuncional" maxlength="3" placeholder="u func" class="form-control txtBox pull-left">
                        <input type="text" name="lUFuncional" id="txtLUFuncional" maxlength="2" placeholder="L u.func" class="form-control txtBox pull-left">
                        <button id="btnNomenAFicha" type="button" class="fillHorizontal btn btn-default btn-md">Buscar</button>
                    </div>
                </div>
                <div class="searchBox">
                    <div class="searchHead">
                        <div class="pull-left"><h5>Busequeda de plancheta por nomenclatura</h5></div>
                        <div class="pull-right fillVertical"><button type="button" id='toggleFormPlanchetaNomen' class="btn btn-succes btn-md fillVertical toggleSearchBody"> Toggle </button></div>
                    </div>
                    <div id='cntFormPlanchetaNomen' class="searchBody">
                        <input type="text" name="circunscripcion" id="txtCircunscripcion" maxlength="1" placeholder="Circ" class="form-control txtBox pull-left txtNomenAFichaReq" required>
                        <input type="text" name="seccion" id="txtSeccion" maxlength="1" placeholder="Sec" class="form-control txtBox pull-left txtNomenAFichaReq" required>
                        <input type="text" name="fraccion" id="txtFraccion" maxlength="3" placeholder="n frac" class="form-control txtBox pull-left">
                        <input type="text" name="letraFraccion" id="txtLFraccion" maxlength="1" placeholder="L frac" class="form-control txtBox pull-left">
                        <input type="text" name="manzana" id="txtManzana" maxlength="3" placeholder="n manz" class="form-control txtBox pull-left" >
                        <input type="text" name="letraManzana" id="txtLManzana" maxlength="1"placeholder="L manz" class="form-control txtBox pull-left">
                        <input type="text" name="parcela" id="txtParcela" maxlength="3" placeholder="n parc" class="form-control txtBox pull-left">
                        <input type="text" name="letraParcela" id="txtLParcela" maxlength="2" placeholder="L parcs" class="form-control txtBox pull-left">
                        <input type="text" name="uFuncional" id="txtUFuncional" maxlength="3" placeholder="u func" class="form-control txtBox pull-left">
                        <input type="text" name="lUFuncional" id="txtLUFuncional" maxlength="2" placeholder="L u.func" class="form-control txtBox pull-left">
                        <button id="btnNomenAFicha" type="button" class="fillHorizontal btn btn-default btn-md">Buscar</button>
